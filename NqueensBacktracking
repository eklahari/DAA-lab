#include <stdio.h>
#include <stdlib.h>
int x[10];
int place(int k,int i)
{
    for(int j=1;j<=k-1;j++)
    {
        if(x[j]==i || (abs(k-j)==abs(i-x[j])))
            return 0;
    }
    return 1;
}
void nqueens(int k,int n)
{
    int i;
    for(i=1;i<=n;i++)
    {  if(place(k,i)==1)
       {    x[k]=i;
            if(k==n)
           {  
              for(int j=1;j<=n;j++)
             {
                 printf("%d ",x[j]);
             }
             printf("\n");
           }
            else
           {
            nqueens(k+1,n);
           }
       }
   }
}

int main()
{   int n;
    printf("enter number of queens");
    scanf("%d",&n);
    printf("%d * %d chessboard nqueen solutions:\n",n,n);
    nqueens(1,n);
}
